{
    "name": "@marek-hanzal/monye.one",
    "private": true,
    "description": "Simple app for analyziing hosehold financial flow (and optimization).",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/marek-hanzal/monye.one.git"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "scripts": {
        "build": "turbo run build --no-daemon",
        "build:no-cache": "turbo run build --no-daemon --no-cache --force",
        "build:types": "turbo run build:types --no-daemon",
        "lint": "turbo run lint --no-daemon",
        "dev": "turbo run dev --no-daemon --parallel",
        "dev-watch": "turbo run dev-watch --no-daemon --parallel",
        "cleanup": "turbo run cleanup --no-daemon --parallel",
        "start": "turbo run start --no-daemon --parallel",
        "prisma-format": "turbo run prisma-format --no-daemon --parallel",
        "prisma-generate": "turbo run prisma-generate --no-daemon --parallel",
        "prisma-migrate-dev": "turbo run prisma-migrate-dev --no-daemon --parallel",
        "prisma-migrate-reset": "turbo run prisma-migrate-reset --no-daemon --parallel",
        "npkill": "npx npkill"
    },
    "devDependencies": {
        "prettier": "^2.8.3",
        "turbo": "^1.7.0",
        "typescript": "^4.9.5"
    }
}
