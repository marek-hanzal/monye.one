{
    "version": "0.5.0",
    "name": "@leight/prisma",
    "description": "Some prisma related helpers.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/leight-core/viv.git"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "sideEffects": false,
    "main": "lib/cjs/index.js",
    "module": "lib/esm/index.js",
    "types": "src/index.ts",
    "files": [
        "lib"
    ],
    "scripts": {
        "build:esm": "tsc -p tsconfig.json -m es2022 --outDir lib/esm",
        "build:cjs": "tsc -p tsconfig.json -m commonjs --outDir lib/cjs",
        "build": "concurrently npm:build:*",
        "dev-watch:esm": "tsc -w -p tsconfig.json -m es2022 --outDir lib/esm --preserveWatchOutput",
        "dev-watch:cjs": "tsc -w -p tsconfig.json -m commonjs --outDir lib/cjs --preserveWatchOutput",
        "dev-watch": "concurrently npm:dev-watch:*",
        "lint": "eslint src",
        "cleanup": "pnpm del-cli .turbo lib tsconfig.tsbuildinfo src/**/*.{d.ts,d.ts.map,js,js.map}"
    },
    "dependencies": {
        "reflect-metadata": "^0.1.13"
    },
    "devDependencies": {
        "@leight/eslint-config-eslint": "workspace:^0.0.0",
        "@leight/tsconfig": "workspace:^0.0.0",
        "@types/node": "^18.11.18",
        "@typescript-eslint/eslint-plugin": "^5.49.0",
        "@typescript-eslint/parser": "^5.49.0",
        "concurrently": "^7.6.0",
        "del-cli": "^5.0.0",
        "eslint": "^8.33.0",
        "eslint-plugin-unicorn": "^45.0.2",
        "typescript": "^4.9.5"
    },
    "bugs": {
        "url": "https://github.com/leight-core/viv/issues"
    },
    "homepage": "https://github.com/leight-core/viv#readme",
    "directories": {
        "lib": "lib"
    }
}
