{
    "version": "0.5.75",
    "name": "@leight/job",
    "description": "Common job stuff.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/leight-core/viv.git"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "sideEffects": false,
    "type": "module",
    "module": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib",
        "src"
    ],
    "scripts": {
        "dev": "turbowatch",
        "generate": "concurrently npm:generate:*",
        "generate:sdk": "tsx src/leight-sdk.ts",
        "build": "concurrently npm:build:*",
        "build:esbuild": "node esbuild.mjs",
        "build:types": "tsc --outDir lib",
        "depcheck": "depcheck",
        "lint": "eslint src",
        "cleanup": "del-cli .turbo lib tsconfig.tsbuildinfo src/**/*.{d.ts,d.ts.map,js,js.map}"
    },
    "dependencies": {
        "@leight/container": "^0.5.64",
        "@leight/filter": "^0.5.59",
        "@leight/prisma": "^0.5.81",
        "@leight/query": "^0.5.63",
        "@leight/react-query": "^0.5.26",
        "@leight/sort": "^0.5.60",
        "@leight/source": "^0.5.63",
        "@leight/winston": "^0.5.62",
        "@leight/zod": "^0.5.9"
    },
    "devDependencies": {
        "@leight/esbuild": "^0.5.42",
        "@leight/eslint-config-eslint": "^0.5.51",
        "@leight/sdk": "^0.5.22",
        "@leight/tsconfig": "^0.5.65",
        "@leight/turbowatch": "^0.5.30",
        "@types/node": "^18.15.0",
        "concurrently": "^7.6.0",
        "del-cli": "^5.0.0",
        "depcheck": "^1.4.3",
        "eslint": "^8.36.0",
        "turbowatch": "^2.3.2",
        "typescript": "^5.0.2"
    },
    "bugs": {
        "url": "https://github.com/leight-core/viv/issues"
    },
    "homepage": "https://github.com/leight-core/viv#readme"
}
