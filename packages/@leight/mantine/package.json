{
    "version": "0.5.46",
    "name": "@leight/mantine",
    "description": "Tools and some wrappers for Mantine UI library.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/leight-core/viv.git"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "sideEffects": false,
