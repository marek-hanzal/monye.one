        "@leight/utils": "^0.5.44",
        "@leight/utils-client": "^0.5.45",
        "@mantine/core": "^6.0.1",
        "@mantine/dropzone": "^6.0.1",
        "@mantine/hooks": "^6.0.1",
        "@mantine/notifications": "^6.0.1",
        "@mantine/nprogress": "^6.0.1",
        "@tabler/icons-react": "^2.10.0",
        "dayjs": "^1.11.7",
