{
    "version": "0.5.0",
    "name": "@leight/mantine",
    "description": "Tools and some wrappers for Mantine UI library.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/leight-core/viv.git"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "sideEffects": false,
    "main": "lib/cjs/index.js",
    "module": "lib/esm/index.js",
    "types": "lib/types/index.d.ts",
    "files": [
        "lib"
    ],
    "scripts": {
        "build:types": "tsc -p tsconfig.json -m es2022 --declaration --emitDeclarationOnly --outDir lib/types",
        "build:esm": "tsc -p tsconfig.json -m es2022 --outDir lib/esm",
        "build:cjs": "tsc -p tsconfig.json -m commonjs --outDir lib/cjs",
        "build": "concurrently npm:build:*",
        "dev-watch:esm": "tsc -w -p tsconfig.json -m es2022 --outDir lib/esm --preserveWatchOutput",
        "dev-watch:cjs": "tsc -w -p tsconfig.json -m commonjs --outDir lib/cjs --preserveWatchOutput",
        "dev-watch": "concurrently npm:dev-watch:*",
        "depcheck": "npx depcheck",
        "lint": "eslint src",
        "cleanup": "pnpm del-cli .turbo lib tsconfig.tsbuildinfo src/**/*.{d.ts,d.ts.map,js,js.map}"
    },
    "dependencies": {
        "@babel/core": "^7.20.12",
        "@emotion/react": "^11.10.5",
        "@leight/file": "workspace:^0.5.0",
        "@leight/file-client": "workspace:^0.5.0",
        "@leight/i18n": "workspace:^0.5.0",
        "@leight/i18n-client": "workspace:^0.5.0",
        "@leight/utils": "workspace:^0.5.0",
        "@leight/utils-client": "workspace:^0.5.0",
        "@mantine/core": "^5.10.2",
        "@mantine/dropzone": "^5.10.2",
        "@mantine/hooks": "^5.10.2",
        "@mantine/nprogress": "^5.10.2",
        "@tabler/icons-react": "^2.1.2",
        "next": "^13.1.6",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "reflect-metadata": "^0.1.13"
    },
    "devDependencies": {
        "@leight/eslint-config-eslint": "workspace:^0.0.0",
        "@leight/tsconfig": "workspace:^0.0.0",
        "@types/node": "^18.11.18",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "@typescript-eslint/eslint-plugin": "^5.50.0",
        "@typescript-eslint/parser": "^5.50.0",
        "concurrently": "^7.6.0",
        "del-cli": "^5.0.0",
        "eslint": "^8.33.0",
        "eslint-plugin-unicorn": "^45.0.2",
        "typescript": "^4.9.5"
    },
    "bugs": {
        "url": "https://github.com/leight-core/viv/issues"
    },
    "homepage": "https://github.com/leight-core/viv#readme",
    "directories": {
        "lib": "lib"
    }
}
