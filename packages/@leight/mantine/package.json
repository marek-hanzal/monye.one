        "@types/node": "^18.15.0",
        "@types/react": "^18.0.28",
        "concurrently": "^7.6.0",
        "del-cli": "^5.0.0",
        "depcheck": "^1.4.3",
        "eslint": "^8.36.0",
        "turbowatch": "^2.3.2",
        "typescript": "^4.9.5"
