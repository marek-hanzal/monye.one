{
    "version": "0.5.6",
    "name": "@leight/viv-client",
    "description": "Proxy export for all @leight client-side libraries.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/leight-core/viv.git"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "sideEffects": false,
    "type": "module",
    "module": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib",
        "src"
    ],
    "scripts": {
        "dev": "turbowatch",
        "build": "concurrently npm:build:*",
        "build:esbuild": "node esbuild.mjs",
        "build:types": "tsc --outDir lib",
        "depcheck": "depcheck",
        "lint": "eslint src",
        "cleanup": "del-cli .turbo lib tsconfig.tsbuildinfo src/**/*.{d.ts,d.ts.map,js,js.map}"
    },
    "dependencies": {
        "@emotion/react": "^11.11.0",
        "@leight/viv": "^0.5.0",
        "@mantine/core": "^6.0.11",
        "@mantine/hooks": "^6.0.11",
        "@mantine/modals": "^6.0.11",
        "@mantine/notifications": "^6.0.11",
        "luxon": "^3.3.0",
        "next": "^13.4.3",
        "next-intl": "^2.14.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "zustand": "^4.3.8"
    },
    "devDependencies": {
        "@leight/esbuild": "^0.5.46",
        "@leight/eslint-config-eslint": "^0.5.55",
        "@leight/tsconfig": "^0.5.69",
        "@leight/turbowatch": "^0.5.34",
        "@types/node": "^18.15.0",
        "@types/react": "^18.2.6",
        "@types/react-dom": "^18.2.4",
        "concurrently": "^7.6.0",
        "del-cli": "^5.0.0",
        "depcheck": "^1.4.3",
        "eslint": "^8.36.0",
        "turbowatch": "^2.3.2",
        "typescript": "^5.0.4"
    },
    "bugs": {
        "url": "https://github.com/leight-core/viv/issues"
    },
    "homepage": "https://github.com/leight-core/viv#readme"
}
