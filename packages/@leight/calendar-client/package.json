{
    "version": "0.5.22",
    "name": "@leight/calendar-client",
    "description": "Clever Calendar based on Mantine UI library.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/leight-core/viv.git"
    },
    "keywords": [
        "calendar",
        "fullcalendar",
        "full-sized",
        "event",
        "mantine",
        "luxon"
    ],
    "engines": {
        "node": ">=16.0.0"
    },
    "sideEffects": false,
    "type": "module",
    "module": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib",
        "src"
    ],
    "scripts": {
        "dev": "turbowatch",
        "build": "concurrently npm:build:*",
        "build:esbuild": "node esbuild.mjs",
        "build:types": "tsc --outDir lib",
        "depcheck": "depcheck",
        "lint": "eslint src",
        "cleanup": "del-cli .turbo lib tsconfig.tsbuildinfo src/**/*.{d.ts,d.ts.map,js,js.map}"
    },
    "dependencies": {
        "@leight/calendar": "^0.5.13",
        "@leight/context-client": "^0.5.72",
        "@leight/filter": "^0.5.60",
        "@leight/i18n": "^0.5.70",
        "@leight/i18n-client": "^0.5.70",
        "@leight/mantine": "^0.5.74",
        "@leight/source": "^0.5.64",
        "@leight/utils": "^0.5.67",
        "@leight/utils-client": "^0.5.68",
        "@leight/zustand": "^0.5.64",
        "@mantine/core": "^6.0.5",
        "@mantine/hooks": "^6.0.5",
        "@tabler/icons-react": "^2.13.0",
        "react": "^18.2.0"
    },
    "devDependencies": {
        "@leight/esbuild": "^0.5.43",
        "@leight/eslint-config-eslint": "^0.5.52",
        "@leight/tsconfig": "^0.5.66",
        "@leight/turbowatch": "^0.5.31",
        "@types/node": "^18.15.0",
        "@types/react": "^18.0.28",
        "concurrently": "^7.6.0",
        "del-cli": "^5.0.0",
        "depcheck": "^1.4.3",
        "eslint": "^8.36.0",
        "turbowatch": "^2.3.2",
        "typescript": "^5.0.2"
    },
    "bugs": {
        "url": "https://github.com/leight-core/viv/issues"
    },
    "homepage": "https://leight-core.github.io/viv/docs/packages/calendar-client"
}
