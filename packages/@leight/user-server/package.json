{
    "version": "0.5.0",
    "name": "@leight/user-server",
    "description": "User stuff used server-side.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/marek-hanzal/monye.one.git"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "sideEffects": false,
    "main": "src/index.ts",
    "types": "src/index.ts",
    "files": [
        "lib"
    ],
    "scripts": {
        "build:esm": "tsc -p tsconfig.json -m es2022 --outDir lib/esm",
        "build:cjs": "tsc -p tsconfig.json --outDir lib/cjs",
        "build": "concurrently npm:build:*",
        "dev-watch:esm": "tsc -w -p tsconfig.json -m es2022 --outDir lib/esm --preserveWatchOutput",
        "dev-watch:cjs": "tsc -w -p tsconfig.json --outDir lib/cjs --preserveWatchOutput",
        "dev-watch": "concurrently npm:dev-watch:*",
        "postinstall": "pnpm run prisma-format && prisma generate",
        "prisma-format": "npx dotenv -c development -- npx prisma format",
        "lint": "eslint src",
        "cleanup": "npx rimraf .turbo lib"
    },
    "dependencies": {
        "@leight/prisma": "*",
        "@leight/user": "*",
        "@leight/utils": "*",
        "@prisma/client": "^4.9.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^4.1.2",
        "tsyringe": "^4.7.0"
    },
    "devDependencies": {
        "@leight/eslint-config-eslint": "*",
        "@leight/tsconfig": "*",
        "@types/node": "^18.11.18",
        "@typescript-eslint/eslint-plugin": "^5.49.0",
        "@typescript-eslint/parser": "^5.49.0",
        "concurrently": "^7.6.0",
        "dotenv-cli": "^7.0.0",
        "eslint": "^8.32.0",
        "eslint-plugin-disable": "^2.0.3",
        "eslint-plugin-unicorn": "^45.0.2",
        "prisma": "^4.9.0",
        "typescript": "^4.9.4"
    },
    "bugs": {
        "url": "https://github.com/marek-hanzal/monye.one/issues"
    },
    "homepage": "https://github.com/marek-hanzal/monye.one#readme",
    "directories": {
        "lib": "lib"
    }
}
