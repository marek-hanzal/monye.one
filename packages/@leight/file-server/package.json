{
    "version": "0.5.78",
    "name": "@leight/file-server",
    "description": "File stuff on server-side.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/leight-core/viv.git"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "sideEffects": false,
    "type": "module",
    "module": "src/index.ts",
    "types": "src/index.ts",
    "files": [
        "src"
    ],
    "scripts": {
        "dev": "turbowatch",
        "generate": "concurrently npm:generate:*",
        "generate:sdk": "tsx src/leight-sdk.ts",
        "depcheck": "depcheck",
        "lint": "eslint src",
        "cleanup": "concurrently npm:cleanup:*",
        "cleanup:sdk": "del-cli src/sdk/**",
        "cleanup:lib": "del-cli .turbo lib tsconfig.tsbuildinfo src/**/*.{d.ts,d.ts.map,js,js.map}"
    },
    "dependencies": {
        "@leight/container": "^0.5.0",
        "@leight/container-server": "^0.5.0",
        "@leight/file": "^0.5.0",
        "@leight/prisma": "^0.5.0",
        "@leight/server": "^0.5.0",
        "@leight/server-server": "^0.5.0",
        "@leight/source": "^0.5.0",
        "@leight/source-server": "^0.5.0",
        "@leight/trpc-source-server": "^0.5.0",
        "@leight/utils": "^0.5.0",
        "depcheck": "^1.4.3",
        "fs-extra": "^11.1.0",
        "mime-detect": "^1.1.4",
        "touch": "^3.1.0"
    },
    "devDependencies": {
        "@leight/esbuild": "^0.5.46",
        "@leight/eslint-config-eslint": "^0.5.55",
        "@leight/sdk": "^0.5.27",
        "@leight/tsconfig": "^0.5.69",
        "@leight/turbowatch": "^0.5.34",
        "@types/fs-extra": "^11.0.1",
        "@types/node": "^18.15.0",
        "@types/touch": "^3.1.2",
        "concurrently": "^7.6.0",
        "del-cli": "^5.0.0",
        "eslint": "^8.36.0",
        "tsx": "^3.12.6",
        "turbowatch": "^2.3.2",
        "typescript": "^5.0.2"
    },
    "bugs": {
        "url": "https://github.com/leight-core/viv/issues"
    },
    "homepage": "https://github.com/leight-core/viv#readme"
}
