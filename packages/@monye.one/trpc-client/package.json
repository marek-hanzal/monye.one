{
    "version": "0.5.0",
    "name": "@monye.one/trpc-client",
    "description": "TRPC client part of the application.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/marek-hanzal/monye.one.git"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "sideEffects": false,
    "main": "src/index.ts",
    "types": "src/index.ts",
    "files": [
        "lib"
    ],
    "scripts": {
        "build:esm": "tsc -p tsconfig.json -m es2022 --outDir lib/esm",
        "build:cjs": "tsc -p tsconfig.json",
        "build": "concurrently npm:build:*",
        "dev:cjs": "tsc -w -p tsconfig.json --preserveWatchOutput",
        "dev:esm": "tsc -w -p tsconfig.json -m es2022 --outDir lib/esm --preserveWatchOutput",
        "dev": "concurrently npm:dev:*",
        "lint": "eslint src"
    },
    "dependencies": {
        "@monye.one/trpc-server": "*",
        "@tanstack/react-query": "^4.23.0",
        "@trpc/client": "^10.9.0",
        "@trpc/next": "^10.9.0",
        "@trpc/react-query": "^10.9.0",
        "@trpc/server": "^10.9.0",
        "next": "^13.1.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "reflect-metadata": "^0.1.13",
        "superjson": "^1.12.2"
    },
    "devDependencies": {
        "@monye.one/eslint-config-eslint": "*",
        "@monye.one/tsconfig": "*",
        "@types/node": "^18.11.18",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "@typescript-eslint/eslint-plugin": "^5.49.0",
        "@typescript-eslint/parser": "^5.49.0",
        "concurrently": "^7.6.0",
        "eslint": "^8.32.0",
        "eslint-plugin-unicorn": "^45.0.2",
        "typescript": "^4.9.4"
    },
    "bugs": {
        "url": "https://github.com/marek-hanzal/monye.one/issues"
    },
    "homepage": "https://github.com/marek-hanzal/monye.one#readme",
    "directories": {
        "lib": "lib"
    }
}
