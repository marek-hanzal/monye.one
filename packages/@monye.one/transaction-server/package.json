{
    "version": "0.5.0",
    "name": "@monye.one/transaction-server",
    "description": "Server-side transaction stuff.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/marek-hanzal/monye.one.git"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "sideEffects": false,
    "main": "src/index.ts",
    "types": "src/index.ts",
    "files": [
        "lib"
    ],
    "scripts": {
        "build:esm": "tsc -p tsconfig.json -m es2022 --outDir lib/esm",
        "build:cjs": "tsc -p tsconfig.json -m commonjs --outDir lib/cjs",
        "build": "concurrently npm:build:*",
        "dev-watch:esm": "tsc -w -p tsconfig.json -m es2022 --outDir lib/esm --preserveWatchOutput",
        "dev-watch:cjs": "tsc -w -p tsconfig.json -m commonjs --outDir lib/cjs --preserveWatchOutput",
        "dev-watch": "concurrently npm:dev-watch:*",
        "postinstall": "pnpm run prisma-format && prisma generate",
        "prisma-format": "npx dotenv -c development -- npx prisma format",
        "lint": "eslint src",
        "cleanup": "pnpm del-cli .turbo lib tsconfig.tsbuildinfo src/**/*.{d.ts,d.ts.map,js,js.map}"
    },
    "dependencies": {
        "@leight/container": "^0.5.0",
        "@leight/prisma": "*",
        "@leight/user": "*",
        "@leight/user-server": "*",
        "@monye.one/transaction": "^0.5.0",
        "@prisma/client": "^4.9.0",
        "reflect-metadata": "^0.1.13",
        "tsyringe": "^4.7.0",
        "zod": "^3.20.2"
    },
    "devDependencies": {
        "@monye.one/eslint-config-eslint": "workspace:^0.0.0",
        "@monye.one/tsconfig": "workspace:^0.0.0",
        "@types/node": "^18.11.18",
        "@typescript-eslint/eslint-plugin": "^5.49.0",
        "@typescript-eslint/parser": "^5.49.0",
        "concurrently": "^7.6.0",
        "del-cli": "^5.0.0",
        "dotenv-cli": "^7.0.0",
        "eslint": "^8.33.0",
        "eslint-plugin-unicorn": "^45.0.2",
        "prisma": "^4.9.0",
        "typescript": "^4.9.5"
    },
    "bugs": {
        "url": "https://github.com/marek-hanzal/monye.one/issues"
    },
    "homepage": "https://github.com/marek-hanzal/monye.one#readme",
    "directories": {
        "lib": "lib"
    }
}
