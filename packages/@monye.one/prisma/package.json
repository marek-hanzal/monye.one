{
    "version": "0.5.0",
    "name": "@monye.one/prisma",
    "description": "Prisma with all models used in the @monye.one, including ones from other packages.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/marek-hanzal/monye.one.git"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "sideEffects": false,
    "type": "module",
    "module": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "build": "concurrently npm:build:*",
        "build:esbuild": "node esbuild.mjs",
        "build:types": "tsc --outDir lib",
        "depcheck": "depcheck",
        "lint": "eslint src",
        "cleanup": "del-cli .turbo lib tsconfig.tsbuildinfo src/**/*.{d.ts,d.ts.map,js,js.map}",
        "prisma:format": "npx dotenv -c development -- npx prisma format",
        "prisma:generate": "npx dotenv -c development -- npx prisma generate"
    },
    "dependencies": {
        "@prisma/client": "4.10.0",
        "reflect-metadata": "^0.1.13",
        "zod": "3.21.0"
    },
    "devDependencies": {
        "@leight/esbuild": "^0.5.5",
        "@leight/eslint-config-eslint": "^0.5.15",
        "@leight/tsconfig": "^0.5.28",
        "@types/node": "^18.14.6",
        "concurrently": "^7.6.0",
        "del-cli": "^5.0.0",
        "depcheck": "^1.4.3",
        "dotenv-cli": "^7.0.0",
        "eslint": "^8.35.0",
        "prisma": "4.10.0",
        "typescript": "^4.9.5",
        "zod-prisma-types": "^2.3.4"
    },
    "bugs": {
        "url": "https://github.com/marek-hanzal/monye.one/issues"
    },
    "homepage": "https://github.com/marek-hanzal/monye.one#readme"
}
