{
    "version": "0.5.0",
    "name": "@monye.one/transaction",
    "description": "Common transaction stuff.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/marek-hanzal/monye.one.git"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "sideEffects": false,
    "type": "module",
    "module": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "dev": "dotenv -e ROARR_LOG=true -- turbowatch",
        "generate": "concurrently npm:generate:*",
        "generate:sdk": "tsx src/leight-sdk.ts",
        "build": "concurrently npm:build:*",
        "build:esbuild": "node esbuild.mjs",
        "build:types": "tsc --outDir lib",
        "depcheck": "depcheck",
        "lint": "eslint src",
        "cleanup": "del-cli .turbo lib tsconfig.tsbuildinfo src/**/*.{d.ts,d.ts.map,js,js.map}"
    },
    "dependencies": {
        "@leight/container": "^0.5.0",
        "@leight/filter": "^0.5.0",
        "@leight/import": "^0.5.27",
        "@leight/query": "^0.5.0",
        "@leight/react-query": "^0.5.0",
        "@leight/sort": "^0.5.27",
        "@leight/source": "^0.5.24",
        "@leight/zod": "^0.5.0",
        "@monye.one/bank": "^0.5.0",
        "@monye.one/prisma": "^0.5.0"
    },
    "devDependencies": {
        "@leight/esbuild": "^0.5.7",
        "@leight/eslint-config-eslint": "^0.5.16",
        "@leight/sdk": "^0.5.0",
        "@leight/tsconfig": "^0.5.30",
        "@leight/turbowatch": "^0.5.0",
        "@types/node": "^18.15.0",
        "concurrently": "^7.6.0",
        "del-cli": "^5.0.0",
        "depcheck": "^1.4.3",
        "dotenv-cli": "^7.1.0",
        "eslint": "^8.36.0",
        "tsx": "^3.12.6",
        "turbowatch": "^2.3.2",
        "typescript": "^5.0.2"
    },
    "bugs": {
        "url": "https://github.com/marek-hanzal/monye.one/issues"
    },
    "homepage": "https://github.com/marek-hanzal/monye.one#readme"
}
