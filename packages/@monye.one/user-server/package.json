    "devDependencies": {
        "@monye.one/eslint-config-eslint": "*",
        "@monye.one/tsconfig": "*",
        "@types/node": "^18.11.18",
        "@typescript-eslint/eslint-plugin": "^5.48.1",
        "@typescript-eslint/parser": "^5.48.2",
        "dotenv-cli": "^6.0.0",
        "eslint": "^8.32.0",
        "eslint-plugin-unicorn": "^45.0.2",
        "prisma": "^4.8.1",
        "typescript": "^4.9.4"
    },
    "bugs": {
        "url": "https://github.com/marek-hanzal/monye.one/issues"
