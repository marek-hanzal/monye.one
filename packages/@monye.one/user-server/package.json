{
    "version": "0.5.0",
    "name": "@monye.one/user-server",
    "description": "User stuff used server-side.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/marek-hanzal/monye.one.git"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "sideEffects": false,
    "main": "src/index.ts",
    "types": "src/index.ts",
    "files": [
        "lib"
    ],
    "scripts": {
        "build": "tsc -p tsconfig.json && tsc -p tsconfig.json -m es6 --outDir lib/esm",
