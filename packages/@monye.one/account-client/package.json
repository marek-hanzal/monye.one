        "@emotion/react": "^11.10.5",
        "@leight/i18n-client": "*",
        "@leight/mantine": "*",
        "@mantine/core": "^5.10.1",
        "@mantine/hooks": "^5.10.1",
        "@mantine/prism": "^5.10.1",
        "@monye.one/ui": "*",
        "@tabler/icons-react": "^2.0.0",
        "i18next": "^22.4.9",
        "next": "^13.1.5",
        "next-auth": "^4.18.10",
        "next-i18next": "^13.0.3",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-i18next": "^12.1.4",
