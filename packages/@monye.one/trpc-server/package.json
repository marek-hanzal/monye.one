{
    "version": "0.5.0",
    "name": "@monye.one/trpc-server",
    "description": "Server-side TRPC stuff.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/marek-hanzal/monye.one.git"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "sideEffects": false,
    "main": "lib/cjs/index.js",
    "module": "lib/esm/index.js",
    "types": "lib/types/index.d.ts",
    "files": [
        "lib"
    ],
    "scripts": {
        "build:esm": "tsc -p tsconfig.json -m es2022 --outDir lib/esm",
        "build:cjs": "tsc -p tsconfig.json -m commonjs --outDir lib/cjs",
        "build:types": "tsc -p tsconfig.json -m es2022 --declaration --emitDeclarationOnly --outDir lib/types",
        "build": "concurrently npm:build:*",
        "dev-watch:esm": "tsc -w -p tsconfig.json -m es2022 --outDir lib/esm --preserveWatchOutput",
        "dev-watch:cjs": "tsc -w -p tsconfig.json -m commonjs --outDir lib/cjs --preserveWatchOutput",
        "dev-watch": "concurrently npm:dev-watch:*",
        "lint": "eslint src",
        "cleanup": "pnpm del-cli .turbo lib tsconfig.tsbuildinfo src/**/*.{d.ts,d.ts.map,js,js.map}"
    },
    "dependencies": {
        "@leight/trpc": "workspace:^0.5.0",
        "@leight/xlsx-import": "workspace:^0.5.0",
        "@leight/xlsx-import-server": "workspace:^0.5.0",
        "@trpc/server": "^10.9.1",
        "devalue": "^4.2.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "reflect-metadata": "^0.1.13",
        "superjson": "^1.12.2",
        "zod": "^3.20.2"
    },
    "devDependencies": {
        "@monye.one/eslint-config-eslint": "workspace:^0.0.0",
        "@monye.one/tsconfig": "workspace:^0.0.0",
        "@types/node": "^18.11.18",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "@typescript-eslint/eslint-plugin": "^5.50.0",
        "@typescript-eslint/parser": "^5.50.0",
        "concurrently": "^7.6.0",
        "del-cli": "^5.0.0",
        "eslint": "^8.33.0",
        "eslint-plugin-disable": "^2.0.3",
        "eslint-plugin-unicorn": "^45.0.2",
        "typescript": "^4.9.5"
    },
    "bugs": {
        "url": "https://github.com/marek-hanzal/monye.one/issues"
    },
    "homepage": "https://github.com/marek-hanzal/monye.one#readme",
    "directories": {
        "lib": "lib"
    }
}
