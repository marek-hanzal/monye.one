{
    "name": "monye.one",
    "version": "0.5.0",
    "private": true,
    "scripts": {
        "build": "npx dotenv -c development -- next build",
        "dev": "next dev",
        "dev-watch": "next dev",
        "postinstall": "pnpm prisma:generate",
        "migration": "npx dotenv -c development -- npx prisma migrate deploy --schema=node_modules/@monye.one/prisma/prisma/schema.prisma",
        "prisma:generate": "npx dotenv -c development -- npx prisma generate --schema=node_modules/@monye.one/prisma/prisma/schema.prisma",
        "prisma:migrate-reset": "npx dotenv -c development -- npx prisma migrate reset --schema=node_modules/@monye.one/prisma/prisma/schema.prisma --force",
        "prisma:migrate-dev": "npx dotenv -c development -- npx prisma migrate dev --schema=node_modules/@monye.one/prisma/prisma/schema.prisma",
        "depcheck": "npx depcheck",
        "lint": "npx dotenv -c development -- next lint",
        "start": "next start",
        "cleanup": "pnpm del-cli .turbo .next tsconfig.tsbuildinfo"
    },
    "dependencies": {
