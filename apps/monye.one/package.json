{
    "name": "monye.one",
    "version": "0.5.0",
    "private": true,
    "scripts": {
        "build": "npx dotenv -c development -- next build",
        "dev": "concurrently npm:dev:*",
        "dev:app": "next dev",
        "dev:docs": "npx @leight/docs",
        "migration": "npx dotenv -c development -- npx prisma migrate deploy --schema=../../packages/@monye.one/prisma/prisma/schema.prisma",
        "prisma:migrate-reset": "npx dotenv -c development -- npx prisma migrate reset --schema=../../packages/@monye.one/prisma/prisma/schema.prisma --force",
        "prisma:migrate-dev": "npx dotenv -c development -- npx prisma migrate dev --schema=../../packages/@monye.one/prisma/prisma/schema.prisma",
        "prisma:db-push": "npx dotenv -c development -- npx prisma db push --accept-data-loss --schema=../../packages/@monye.one/prisma/prisma/schema.prisma",
        "depcheck": "depcheck",
        "lint": "npx dotenv -c development -- next lint",
        "start": "npm run migration && next start",
        "cleanup": "del-cli .turbo .next tsconfig.tsbuildinfo"
    },
    "dependencies": {
        "@leight/mantine": "^0.5.0",
        "@mantine/core": "^6.0.1",
        "@mantine/hooks": "^6.0.1",
        "@mantine/next": "^6.0.1",
        "next": "^13.4.3",
        "next-auth": "^4.20.1",
        "next-intl": "^2.14.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "server-only": "^0.0.1"
    },
    "devDependencies": {
        "@leight/tsconfig": "^0.5.30",
        "@leight/docs": "^0.5.3",
        "@types/node": "^18.15.0",
        "@typescript-eslint/eslint-plugin": "^5.54.1",
        "@typescript-eslint/parser": "^5.54.1",
        "autoprefixer": "^10.4.14",
        "del-cli": "^5.0.0",
        "depcheck": "^1.4.3",
        "dotenv-cli": "^7.1.0",
        "eslint": "^8.36.0",
        "eslint-config-next": "^13.2.4",
        "postcss": "^8.4.21",
        "turbowatch": "^2.3.2",
        "typescript": "^5.0.4"
    },
    "ct3aMetadata": {
        "initVersion": "6.6.0"
    }
}
